fpsStroke.Parent = fpsLabel

local ShowFPS = false
local lastTime = tick()
local frameCount = 0

RunService.RenderStepped:Connect(function()
    -- Ping
    if ShowPing then
        local ping = math.floor(LocalPlayer:GetNetworkPing() * 1000)
        pingLabel.Text = "Ping: "..ping.." ms"
        if ping > 100 then
            pingLabel.TextColor3 = Color3.fromRGB(255,0,0) -- ‡πÅ‡∏î‡∏á
        elseif ping >= 70 and ping <= 80 then
            pingLabel.TextColor3 = Color3.fromRGB(255,255,0) -- ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á
        elseif ping < 60 then
            pingLabel.TextColor3 = Color3.fromRGB(0,255,0) -- ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß
        else
            pingLabel.TextColor3 = Color3.fromRGB(255,165,0) -- ‡∏™‡πâ‡∏°
        end
    end

    -- FPS
    if ShowFPS then
        frameCount = frameCount + 1
        local currentTime = tick()
        if currentTime - lastTime >= 1 then
            fpsLabel.Text = "FPS: " .. frameCount
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ FPS
            if frameCount < 30 then
                fpsLabel.TextColor3 = Color3.fromRGB(255,0,0) -- ‡πÅ‡∏î‡∏á
            elseif frameCount >= 30 and frameCount <= 60 then
                fpsLabel.TextColor3 = Color3.fromRGB(255,255,0) -- ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á
            else
                fpsLabel.TextColor3 = Color3.fromRGB(0,255,0) -- ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß
            end
            frameCount = 0
            lastTime = currentTime
        end
    end
end)

