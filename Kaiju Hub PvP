    Title = "üõ°Ô∏èSafe Players",
    Multi = true,
    AllowNone = true,
    Values = UpdatePlayerNames(),
    Value = {},
    Callback = function(selected)
        IgnorePlayers = {}
        for _, name in ipairs(selected) do
            table.insert(IgnorePlayers, name)
        end
        print("Safe Players Updated: " .. table.concat(IgnorePlayers, ", "))
    end
})

